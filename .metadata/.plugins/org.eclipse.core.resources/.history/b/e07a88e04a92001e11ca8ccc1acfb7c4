package edu.kh.jdbc.board.model.vo;

import java.sql.Date;

public class Board {
	
	// 게시글 목록 조회 
	private int boardNo;
	private String boardTitle;
	private Date createDate;
	private int readCount;
	private String memberName;
	private int replyCount;
	
	// 게시글 상세 조회
	private String boardContent;
	
	


}
